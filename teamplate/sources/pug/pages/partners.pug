extends  ../_skeleton

block variables
	- var activeCategory = 'partners'
	- var pageName = 'Partners'

block page 
	// Page Header
	include ../sections/_header-classic

	+breadcrumbs({
		category: activeCategory,
		path: [pageName],
		img: 'images/icon-lists-title-bg.jpg'
	})

	section.section.section-lg.bg-default.text-center
		.container
			.row.row-30.row-xl-50
				.col-12
					h2.heading-with-text-bg
						| Our Partners
						span.block-background-text.text-decorative-black-20.big Partners
				.col-sm-6.col-lg-3
					a(href='#').box-sponsor.box-sponsor-modern.wow-outer
						+png('logo-inverse-1-270x121')
				.col-sm-6.col-lg-3
					a(href='#').box-sponsor.box-sponsor-modern.wow-outer
						+png('logo-inverse-2-270x121')
				.col-sm-6.col-lg-3
					a(href='#').box-sponsor.box-sponsor-modern.wow-outer
						+png('logo-inverse-3-270x121')
				.col-sm-6.col-lg-3
					a(href='#').box-sponsor.box-sponsor-modern.wow-outer
						+png('logo-inverse-4-270x121')


				.col-12
					a(href='#').button.button-lg.button-primary become a partner
	// Page Footer
	include ../sections/_footer-minimal
